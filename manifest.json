{
  "manifest_version": 2,
  "name": "Lessit",
  "version": "1.0",
  "description": "A extension to reduce reddit use.",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.reddit.com/*"],
      "js": ["lessit.js"],
      "css": ["lessit.css"]
    }
  ]

}
